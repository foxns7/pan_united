package challenge2;

import java.util.ArrayList;



public class challenge2 { // inner class
	
	static int lol(int[] data) {
		Manager manager = new Manager();
		
		for(int number: data) {
			if(manager.getFirstGroupSum() != number) {
				manager.add(number);
			}
		}
		return manager.getGroupSize();
	}

	public static void main(String[] args) {
		int[] data = {1,4,1,3,1};
		lol(data);
	}

}
