public class OptimalBreakTime {
	
	private static int intg[];
    
    public static void setIntg(int intgr[]) { 
    	intg = intgr; 
    }
    	
    public static int[] getIntg() {
    	return intg;
    }
    
    
    
    public static void main (String[] args) {
       int intg[] = {1, 4, 1, 3, 1};
       OptimalBreakTime.setIntg(intg);
       int intgr[] = OptimalBreakTime.getIntg();
 	   System.out.println("intgr  '"+intgr+"'");
 	   
 	  
    }

}
