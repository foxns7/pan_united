package challenge2;

import java.util.ArrayList;

public class challenge2 {
	
	static ArrayList<ArrayList<Integer>> listOfSums = new ArrayList<ArrayList<Integer>>();

	static int lol(int[] data) {
		int count = 0;
		int smallestSum = data[0];
		for(int number: data) {
			if(smallestSum!=number) {
				smallestSum+=number;
			}
		}
		return listOfSums.size();
	}
	
	public static void main(String[] args) {
		int[] data = {1,4,1,3,1};
		lol(data);
	}

}
