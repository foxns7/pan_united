    static String checkSpaces (String inputstr) {
    	
    	String[] str = inputstr.split("\\s+");
    	for (int i = 0; i < str.length; i++) {
			System.out.println(str[i]);
		}
    	
    	return inputstr;
    }
		
	static int countWords (String inputstr) {
	    int i = 0;	
		return i;
	}
	
	
	
   public static void main (String[] args) {
	   Challenge.setInputStr("This Is A Test");
