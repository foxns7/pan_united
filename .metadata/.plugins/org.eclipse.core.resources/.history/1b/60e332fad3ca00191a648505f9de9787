package challenge2;

public class challenge2 {

	static int lol(int[] data) {
		int count = 0;
		int smallestSum = data[0];
		for(int number: data) {
			if(smallestSum!=number) {
				smallestSum+=number;
			}
		}
		return 0;
	}
	
	public static void main(String[] args) {
		int[] data = {1,4,1,3,1};
		lol(data);
	}

}
