
public class Challenge {
    
    private static String inputstr;
    
    public static void setInputStr(String inputstr) { }
    	
    public static String getInputStr() {
    	return inputstr;
    }
    
    static boolean checkSpaces (String inputstr) {
	    boolean hasSpace = false; 
	    if (inputstr != null) {
	        for (int i = 0; i < inputstr.length(); i++) {
	            if (inputstr.charAt(i) == ' ') {
	            	hasSpace = true;
	            }
	        }
	    }
	    return hasSpace;
    }
		
	static int countWords (String inputstr) {
		
	}
	
	
	
   public static void main (String[] args) {
	   Challenge.setInputStr("HiThereIAmHamiz");
	   String inputStr = Challenge.getInputStr();
   }
	
}
