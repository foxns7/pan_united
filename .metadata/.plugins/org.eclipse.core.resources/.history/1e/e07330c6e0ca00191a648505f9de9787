package challenge2;

import java.util.ArrayList;



public class challenge2 { // inner class
	
	static Object getBreakTime(int[] data) {
		Manager manager = new Manager();
		
		for(int number: data) {
			manager.add(number);
		}
		return manager.getListSize();
	}

	public static void main(String[] args) {
		int[] data = {1,4,1,3,1,5};
		System.out.println(getBreakTime(data));
	}

}
